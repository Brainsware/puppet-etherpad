{
  "name": "brainsware-etherpad",
  "version": "0.1.0",
  "author": "brainsware",
  "summary": "Install and configure Etherpad",
  "license": "Apache-2.0",
  "source": "https://gitlab.com/brainsware-infra/puppet-etherpad",
  "project_page": "https://gitlab.com/brainsware-infra/puppet-etherpad",
  "issues_url": "https://gitlab.com/brainsware-infra/puppet-etherpad/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppetlabs-vcsrepo",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ]
}
